    {\value{cnt@board@file}<\numexpr \board@temp@curstopfilenum+1\relax}%
    {\board@do@showfield{\number\value{cnt@board@file}}{\number\value{cnt@board@rank}}%
     \addtocounter{cnt@board@file}{1}%
     }%
   \addtocounter{cnt@board@rank}{-1}}}%

