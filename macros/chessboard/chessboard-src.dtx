   {}%
   {\def\board@val@castling{#3}}%
  \ifthenelse{\equal{#4}{X}}%
   {}%
   {\def\board@val@enpassant{#4}}%
  \ifthenelse{\equal{#5}{X}}%
   {}%
   {\def\board@val@halfmove{#5}}%
  \ifthenelse{\equal{#6}{X}}%
   {}%
   {\def\board@val@fullmove{#6}}%
    }%

