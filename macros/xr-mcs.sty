\ProvidesPackage{xr-mcs}

\RequirePackage{xr-hyper}

\long\def\XR@test#1#2#3#4\XR@{%
  \ifx#1\newlabel
      \expandafter\protected@xdef\csname r@\XR@prefix#2\endcsname
       {\XR@addURL{#3}}%
  \else\ifx#1\bibcite
%% Import bibliography labels, but without the prefix.
      \expandafter\protected@xdef\csname b@#2\endcsname
          {\noexpand\hyper@@link [cite]{\XR@URL}{cite.#2}{#3}}%
  \else\ifx#1\@input
      \edef\XR@list{\XR@list\filename@area#2\relax}%
  \fi\fi\fi
  \ifeof\@inputcheck\expandafter\XR@aux
  \else\expandafter\XR@read\fi}

\endinput